{
  "name": "foodshare-mern-app",
  "version": "1.0.0",
  "description": "MERN Stack Food Sharing Application with JWT Authentication",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm run dev",
    "client": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "install-server": "cd backend && npm install",
    "install-client": "cd frontend && npm install",
    "install-all": "npm run install-server && npm run install-client",
    "test": "node tests/integration/cv-integration.test.js",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test",
    "start-cv": "python scripts/start_ai_service.py",
    "start-ai": "python scripts/start_ai_service.py",
    "start-ai-windows": "scripts\\start-enhanced-ai.bat",
    "test-services": "node scripts/simple-test.js",
    "lint": "cd frontend && npm run lint && cd ../backend && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kirtann8/foodshare-mern-app.git"
  },
  "keywords": [
    "mern",
    "react",
    "nodejs",
    "mongodb",
    "express",
    "jwt",
    "authentication",
    "food-sharing"
  ],
  "author": "Kirtan Patel",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}
